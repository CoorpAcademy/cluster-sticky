{
    "bitwise": false,
    "browser": true,
    "curly": false,
    "devel": true,
    "eqeqeq": true,
    "eqnull": true,
    "forin": true,
    "immed": true,
    "indent": 4,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxdepth": 4,
    "maxlen": 140,
    "maxparams": false,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "plusplus": false,
    "quotmark": false,
    "smarttabs": false,
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": "vars",

    "globals": {
        "it": true,
        "fit": true,
        "xdescribe": true,
        "ddescribe": true,
        "fdescribe": true,
        "xit": true
    },
    "-W018": true,
    "-W024": true,
    "-W101": true,
    "-W086": true,
    "-W030": true
}
